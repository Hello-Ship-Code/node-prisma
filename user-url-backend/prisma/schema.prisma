generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model users {
  id        String  @id @default(auto()) @map("_id") @db.ObjectId
  firstName String?
  lastName  String?
  email     String  @unique
  gender    String
  jobTitle  String
}

model urls {
  id           String @id @default(auto()) @map("_id") @db.ObjectId
  shortId      String
  redirectUrl  String
  visitHistory Json
}
